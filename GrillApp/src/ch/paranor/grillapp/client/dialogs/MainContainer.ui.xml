<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.northPanel {
		background-color: blue;
		text-align: center;
		font-size: 40px;
		line-height: 2;
		font-family: "Copperplate Gothic Bold", serif;
		font-style: oblique;
		color: white;
	}
	
	.centerPanel {
		background-color: #FFF8DC;
	}
	
	.westPanel {
		background-color: green;
	}
	
	.southPanel {
		background-color: blue;
		text-align: center;
		font-size: 14px;
		line-height: 4;
	}
	</ui:style>

	<g:DockLayoutPanel unit='EM'>
		<g:north size='7'>
			<g:HeaderPanel styleName="{style.northPanel}" ui:field="northPanel">
				<g:Label>Paranor Grillapp</g:Label>
			</g:HeaderPanel>
		</g:north>

		<g:center size='5'>
			<g:HeaderPanel styleName="{style.centerPanel}"
				ui:field="centerpanel">
				
			</g:HeaderPanel>

		</g:center>
 
	<g:south size='5'>
			<g:HeaderPanel styleName="{style.southPanel}" ui:field="southPanel">
				<g:Label>Neshanthan Nelson</g:Label>
			</g:HeaderPanel>
		</g:south>

		<g:west size='20'>
		<g:HeaderPanel styleName="{style.westPanel}"
				ui:field="westpanel">
			</g:HeaderPanel>
		</g:west>

	

		<!-- <g:west size='20'> <g:HeaderPanel styleName="{style.westPanel}" ui:field="westpanel"> 
			<g:Label>West</g:Label> </g:HeaderPanel> </g:west> -->

	</g:DockLayoutPanel>

</ui:UiBinder> 