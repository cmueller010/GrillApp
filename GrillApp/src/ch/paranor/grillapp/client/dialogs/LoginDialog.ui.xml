<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>

		<Label>Login to Grillapp </Label>
		<Table>
			<tr>
				<td>
					<Label>Benutzername: </Label>
				</td>
				<td>
					<g:TextBox ui:field="benutzername"></g:TextBox>
				</td>
			</tr>
			<tr>
				<td>
					<Label>Passwort: </Label>
				</td>
				<td>
					<g:PasswordTextBox ui:field="passwort"></g:PasswordTextBox>
				</td>
			</tr>
			<tr>
				<td>
				</td>
				<td>
					<g:Button text="Anmelden" ui:field="anmeldenButton"></g:Button>
				</td>
			</tr>
		</Table>



	</g:HTMLPanel>
</ui:UiBinder> 