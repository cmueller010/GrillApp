<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.table {
		border: 1px solid black;
		margin-left: 35%;
		margin-top: 5%;
	}
	

	</ui:style>

	<g:HTMLPanel>

			<Table class='{style.table}'>
				<tr>
					<th>
						<g:Label>Aktion</g:Label>
					</th>
					<th>
						<g:Label>Titel</g:Label>
					</th>
					<th>
						<g:Label>findet statt am</g:Label>
					</th>
					<th>
						<g:Label>Anmeldung bis</g:Label>
					</th>
					<th>
						<g:Label>Status</g:Label>
					</th>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="anlassAction">Anmelden | Abmelden | Bearbeiten </g:Label>
					</td>
					<td>
						<g:Label ui:field="anlassTitel"></g:Label>
					</td>
					<td>
						<g:Label ui:field="anlassFindetStatt">2. Februar 2015</g:Label>
					</td>
					<td>
						<g:Label ui:field="anlassAnmeldung">26. Januar 2015</g:Label>
					</td>
					<td>
						<g:Label ui:field="anlassStatus">Offen</g:Label>
					</td>
				</tr>

			</Table>
			<g:Button ui:field="organisierenButton">Anlass Organisieren</g:Button>
		
	</g:HTMLPanel>

</ui:UiBinder> 